<h1>All Users</h1>
<table class="users">
    <tr>
      <th>Username</th>
      <th><%#votes%></th>
      <th>Joined</th> 
    </tr>
    <%@users.each do |user|%>
      <tr>
        <td><%=link_to user.username, user_path(user)%></td>
        <td><%#votes%></td>
        <td><%=user.created_at%></td>
        <td> </td>
      </tr>
    <%end%>
  </table>